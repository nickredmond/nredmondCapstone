package threading;

import java.util.List;

public class DeltaValues {
	private List<Integer[][]> inputDeltas, outputDeltas;
	private List<Integer[][][]> hiddenDeltas;
	
	private int[][] inputTotal, outputTotal;
	private int[][][] hiddenTotal;

	public List<Integer[][]> getInputDeltas() {
		return inputDeltas;
	}

	public void setInputDeltas(List<Integer[][]> inputDeltas) {
		this.inputDeltas = inputDeltas;
	}

	public List<Integer[][][]> getHiddenDeltas() {
		return hiddenDeltas;
	}

	public void setHiddenDeltas(List<Integer[][][]> hiddenDeltas) {
		this.hiddenDeltas = hiddenDeltas;
	}

	public List<Integer[][]> getOutputDeltas() {
		return outputDeltas;
	}

	public void setOutputDeltas(List<Integer[][]> outputDeltas) {
		this.outputDeltas = outputDeltas;
	}

	public int[][] getInputTotal() {
		return inputTotal;
	}

	public void setInputTotal(int[][] inputTotal) {
		this.inputTotal = inputTotal;
	}
}
