package appTest;

import java.util.ArrayList;
import java.util.List;

import org.junit.Test;

import ui.TrainingDataNameAssigner;

public class NameAssignerTest {

	@Test
	public void test() {
		List<Integer> takenNumbers1 = new ArrayList<Integer>();
		int result1 = TrainingDataNameAssigner.getFirstAvailableNumber(takenNumbers1);
		
		org.junit.Assert.assertEquals(result1, 0);
		
		List<Integer> takenNumbers2 = new ArrayList<Integer>();
		takenNumbers2.add(1);
		takenNumbers2.add(2);
		takenNumbers2.add(4);
		takenNumbers2.add(5);
		int result2 = TrainingDataNameAssigner.getFirstAvailableNumber(takenNumbers1);
		
		org.junit.Assert.assertEquals(result2, 0);
		
		List<Integer> takenNumbers3 = new ArrayList<Integer>();
		int result3 = TrainingDataNameAssigner.getFirstAvailableNumber(takenNumbers1);
		
		org.junit.Assert.assertEquals(result3, 0);
	}

}
