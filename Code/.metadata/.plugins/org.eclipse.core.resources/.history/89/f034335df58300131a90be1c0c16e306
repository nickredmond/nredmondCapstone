package app;

public class GeneralMath {
	private final int EXPANSION_ORDER = 35;
	
	public static float arcTangent(float value){
		return 0.0f;
	}
	
	public static int factorial(int value){
		int factorial = 0;

		if (value == 1){
			factorial = 1;
		}
		else factorial = value * factorial(value - 1);
		
		return factorial;
	}
}
