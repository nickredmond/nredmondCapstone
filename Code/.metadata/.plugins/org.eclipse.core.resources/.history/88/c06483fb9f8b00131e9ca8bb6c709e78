package app;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;

public class DictionaryFactory {
	public static final String DICTIONARY_FILEPATH = "dictionary.txt";
	
	public static IDictionary getDictionary(){
		IDictionary dictionary = new TextFileDictionary();
		
		try {
			BufferedReader reader = new BufferedReader(new FileReader(new File(DICTIONARY_FILEPATH)));
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
	}
}
