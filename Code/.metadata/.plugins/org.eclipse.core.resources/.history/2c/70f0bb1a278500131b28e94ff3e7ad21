package featureExtraction;

public class FeaturePoint {
	private int x, y;
	private FeatureType type;
	
	public FeaturePoint(int x, int y, FeatureType type){
		this.type = type;
		this.x = x;
		this.y = y;
	}
	
	public FeatureType getType(){
		return type;
	}

	public int x() {
		return x;
	}

	public int y() {
		return y;
	}
}
