package appTest;

import neuralNetwork.NeuralNetwork;

public class MainTest {

	public static void main(String[] args) {
		NeuralNetwork network = new NeuralNetwork(2, 3, 3, 5, true);
		network.setWeightForNeuron(1, 0, 1, 0.98f);
		network.setWeightForNeuron(1, 0, 2, 0.98f);
		network.setWeightForNeuron(1, 0, 3, 0.98f);
		network.setWeightForNeuron(1, 1, 1, 0.98f);
		network.setWeightForNeuron(1, 1, 2, 0.98f);
		network.setWeightForNeuron(1, 1, 3, 0.98f);
		network.setWeightForNeuron(1, 2, 1, 0.98f);
		network.setWeightForNeuron(1, 2, 2, 0.98f);
		network.setWeightForNeuron(1, 2, 3, 0.98f);
		network.getOutputForInput(new float[]{0.1f, 0.7f});
	}

}
